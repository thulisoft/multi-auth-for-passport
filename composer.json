{
    "name": "thulisoft/multi-auth-for-passport",
    "description": "Add support to multi-auth on Laravel Passport",
    "type": "library",
    "require": {
        "php": "^7.2",
        "laravel/passport": "^10.1"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "mockery/mockery": "^1.0",
        "orchestra/database": "^4.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Thulisoft\\MultiAuthForPassport\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Thulisoft\\MultiAuthForPassport\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
